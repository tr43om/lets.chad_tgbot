{
  "name": "gpt-bot",
  "version": "1.0.0",
  "main": "build/main.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "ts:watch": "tsc -w",
    "node:dev": "cross-env NODE_ENV=development nodemon",
    "start": "concurrently \"yarn ts:watch\" \"yarn node:dev\" cross-env NODE_ENV=production",
    "build": "tsc --build && cross-env NODE_ENV=production node build/index.js"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node": "^20.1.2",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "axios": "^1.4.0",
    "config": "^3.3.9",
    "fluent-ffmpeg": "^2.1.2",
    "openai": "^3.2.1",
    "telegraf": "^4.12.2",
    "ts-node": "^10.9.1"
  }
}
